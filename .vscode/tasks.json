{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"options": {
				"cwd": "examples/treasure_hunter"
			},
			"command": "run",
			"args": [
				"--features",
				"ggsdk/dynamic"
			],
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: run example treasure_hunter"
		},
		{
			"type": "cargo",
			"options": {
				"cwd": "examples/gl"
			},
			"command": "run",
			"args": [
				"--features",
				"ggsdk/dynamic"
			],
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: run example gl"
		},
		{
			"type": "shell",
			"command": [
				"cargo publish -p ggsdk_internal &&",
				"cargo publish -p ggsdk_dynamic --no-verify &&",
				"cargo publish"
			],
			"label": "cargo: publish",
			"problemMatcher": []
		}
	]
}