		},
		{
			"type": "cargo",
			"options": {
				"cwd": "examples/gl"
			},
			"command": "run",
			"args": ["--features", "ggsdk/dynamic"],
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: run example gl"
		}
	]
}